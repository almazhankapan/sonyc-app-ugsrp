     setLanguage(e.target.value)\n                console.log(e.target.value)\n                if (e.target.value == 'Python') {\n                  setTheme('colorforth')\n                } else {\n                  setTheme('eclipse')\n                }\n              }}\n            >\n              <option value=\"C++\">C++</option>\n              <option value=\"Python\">Python</option>\n            </select>\n          </div>\n          <br />\n\n          <EditorA\n            language={language}\n            displayName={language}\n            value={code}\n            theme={theme}\n            onChange={setCode}\n          />\n          <br />\n          <div className=\"btn-submit\">\n            <button onClick={handleSubmit}>Run</button>\n          </div>\n          <div className=\"pane\">\n            <br /> <br /> <br />\n            <iframe\n              className=\"pane-frame\"\n              srcDoc={output}\n              title=\"output\"\n              color=\"white\"\n              sandbox=\"allow-scripts\"\n              frameBorder=\"1\"\n              width=\"70%\"\n              height=\"100%\"\n            />\n          </div>\n        </div>\n      </div>\n      <Footer />\n    </>\n  )\n}\n\nexport default CompilerPyCpp\n","/Users/almazhan/Desktop/code-editor/client/src/pages/EditorHtmlJS.js",["59","60"],"import React from 'react'\nimport Header from '../components/Header'\nimport Footer from '../components/Footer'\nimport { useState, useEffect } from 'react'\nimport axios from 'axios'\nimport { Link } from 'react-router-dom'\nimport Editor from './Editor'\nimport './EditorHtmlJS.css'\nfunction EditorHtmlJS() {\n  const [html, setHtml] = useState('')\n  const [js, setJs] = useState('')\n  const [css, setCss] = useState('')\n  const [srcDoc, setSrcDoc] = useState('')\n\n  useEffect(() => {\n    const timeout = setTimeout(() => {\n      setSrcDoc(`\n        <html>\n          <body>${html}</body>\n          <style>${css}</style>\n          <script>${js}</script>\n        </html>\n      `)\n    }, 250)\n\n    return () => clearTimeout(timeout)\n  }, [html, css, js])\n  return (\n    <>\n      <Header />\n      <div className=\"home-code\">\n        <h2 className=\"header-text-cpp\">\n          Write and Render your HTML, Javascript, CSS code!\n        </h2>\n        <div className=\"pane top-pane\">\n          <br /> <br /> <br />\n          <Editor\n            language=\"xml\"\n            displayName=\"HTML\"\n            value={html}\n            onChange={setHtml}\n          />\n          <Editor\n            language=\"css\"\n            displayName=\"CSS\"\n            value={css}\n            onChange={setCss}\n          />\n          <Editor language=\"js\" displayName=\"JS\" value={js} onChange={setJs} />\n        </div>\n        <div className=\"pane\">\n          <br /> <br /> <br />\n          <iframe\n            srcDoc={srcDoc}\n            title=\"output\"\n            sandbox=\"allow-scripts\"\n            frameBorder=\"0\"\n            width=\"100%\"\n            height=\"100%\"\n          />\n        </div>\n      </div>\n      <Footer />\n    </>\n  )\n}\n\nexport default EditorHtmlJS\n","/Users/almazhan/Desktop/code-editor/client/src/components/Header.js",["61","62","63"],"import React, { useEffect, useState, useRef } from 'react'\nimport 'react-sticky-header/styles.css'\nimport './Header.css'\nimport NavBar from './NavBar'\n\nconst Header = () => {\n  return (\n    <>\n      <NavBar />\n    </>\n  )\n}\n\nexport default Header\n","/Users/almazhan/Desktop/code-editor/client/src/components/Footer.js",["64","65"],"import React from 'react'\nimport './Footer.css'\nimport { Link } from 'react-router-dom'\nimport trakt from '../images/logo.png'\n\nfunction Footer() {\n  return (\n    <footer>\n      <div className=\"footer-container\">\n        <a href=\"https://hello-air.com/\" className=\"footer-links\">\n          Air Concepts\n        </a>\n      </div>\n    </footer>\n  )\n}\n\nexpo